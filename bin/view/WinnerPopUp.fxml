<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" fillWidth="false" prefHeight="800.0" prefWidth="933.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.WinnerController">
   <children>
      <VBox alignment="CENTER" prefHeight="400.0" prefWidth="500.0">
         <children>
            <TextField fx:id="winnerField" alignment="CENTER" editable="false" text="The winner is ">
               <font>
                  <Font size="25.0" />
               </font>
            </TextField>
            <Button fx:id="playAgainButton" mnemonicParsing="false" onAction="#playAgain" prefHeight="60.0" prefWidth="250.0" text="Play again">
               <font>
                  <Font size="25.0" />
               </font>
               <VBox.margin>
                  <Insets top="75.0" />
               </VBox.margin>
            </Button>
            <Button fx:id="menuButton" mnemonicParsing="false" onAction="#goToMenu" prefHeight="60.0" prefWidth="250.0" text="Menu">
               <font>
                  <Font size="25.0" />
               </font>
               <VBox.margin>
                  <Insets top="25.0" />
               </VBox.margin>
            </Button>
         </children>
      </VBox>
   </children>
</VBox>
